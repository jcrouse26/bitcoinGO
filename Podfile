# Uncomment the next line to define a global platform for your project
platform :ios, '11.3'

inhibit_all_warnings!

target 'bitcoinGO' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for bitcoinGO
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/DynamicLinks'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'GoogleMaps'
  pod 'GooglePlaces' 


  target 'bitcoinGOTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'bitcoinGOUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

